<%= form_for @student do |f| %>
<%= f.label 'First name', :class => 'label'%>
<%= f.text_field :first_name, :class => "form-control"%><br>
<%= f.label 'Last name', :class => 'label'%>
<%= f.text_field :last_name,:class => "form-control" %><br>
<%= f.label 'Age', :class => 'label'%>
<%= f.number_field :age,:class => "form-control"%><br>
<%= f.label 'Education', :class => 'label'%>
<%= f.text_field :education,:class => "form-control"%><br>
<%= f.label 'Pick a cohort', :class => "label" %>
<%= f.collection_select :cohort_id, Cohort.all , :id, :name, {include_blank: true}, :class => "form-control" %><br>
<%= f.submit :class=> "btn" %>
<% end %>
